<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import jfxtras.labs.samples.repeatagenda.scene.control.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import jfxtras.scene.control.*?>
<?import jfxtras.labs.icalendaragenda.internal.scene.control.skin.agenda.base24hour.*?>

<fx:root type="javafx.scene.control.TabPane" fx:id="editDisplayableTabPane" prefHeight="600.0" prefWidth="400.0" styleClass="background3" stylesheets="@resources/PopupStyles.css" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
  <tabs>
    <Tab fx:id="descriptiveTab" closable="false" text="%appointment">
      <content>
        <AnchorPane fx:id="descriptiveAnchorPane" >
		  <EditVEventDescriptiveVBox fx:id="editVEventDescriptive"/>
        </AnchorPane>
      </content>
    </Tab>
    <Tab fx:id="recurrenceRuleTab" closable="false" text="%repeatable">
         <content>
            <AnchorPane>
               <children>
                  <EditRecurrenceRuleVBox fx:id="repeatable"/>
                  <HBox alignment="CENTER" spacing="5.0" AnchorPane.bottomAnchor="7.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                     <children>
                        <Button fx:id="cancelRepeatButton" mnemonicParsing="false" onAction="#handleCancelButton" text="%cancel" />
                        <Button fx:id="saveRepeatButton" layoutX="170.0" layoutY="10.0" mnemonicParsing="false" onAction="#handleRepeatSave" text="%save" />
                     </children>
                  </HBox>
               </children>
            </AnchorPane>
         </content>
    </Tab>
  </tabs>
</fx:root>
