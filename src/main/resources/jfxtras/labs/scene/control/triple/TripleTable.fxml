<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>

<fx:root spacing="10.0" styleClass="bordered-titled-border" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="table" editable="true" prefHeight="120.0" prefWidth="670.0">
         <columns>
            <TableColumn fx:id="dataColumn" text="data" />
            <TableColumn fx:id="nameColumn" text="%name" />
            <TableColumn fx:id="primaryColumn" text="%primary" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="deleteButton" disable="true" mnemonicParsing="false" text="%delete" />
   </children>
</fx:root>
